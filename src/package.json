{
  "name": "ptp_movie_checker",
  "version": "0.1.0",
  "description": "A comprehensive movie management system for PassThePopcorn users. Features include checking movie existence, managing missing, ignored, and found movies, and providing a ViolentMonkey script for seamless integration with various movie sites. Includes a proxy server to avoid CORS issues.",
  "main": "app.js",
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "build": "node_modules/.bin/webpack --mode production",
    "watch": "node_modules/.bin/webpack --watch --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" --ignore-path .prettierignore"
  },
